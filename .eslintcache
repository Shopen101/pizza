Popup activeSortType={sortBy.type} items={sortItems} onClickSortType={onSelectSortType} />\r\n            </div>\r\n            <h2 className=\"content__title\">Все пиццы</h2>\r\n            <div className=\"content__items\">\r\n                {isLoaded\r\n                    ? items.map(obj => \r\n                    <PizzaBlock \r\n                        onClickAddPizza={handleAddPizzaToCart} \r\n                        key={obj.id} \r\n                        addedCount={cartItems[obj.id] && cartItems[obj.id].items.length}\r\n                        {...obj} \r\n                    />)\r\n                    : Array(12)\r\n                        .fill(0)\r\n                        .map((_, index) => <PizzaLoadingBlock key={index} />)}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home\r\n","C:\\html\\JS\\ReactJS\\react-pizza\\src\\pages\\index.js",[],["104","105"],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\pages\\Cart.jsx",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\store.js",[],["106","107"],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\reducers\\pizzas.js",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\reducers\\filters.js",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\reducers\\index.js",[],["108","109"],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\action\\pizzas.js",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\action\\filters.js",[],["110","111"],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\components\\PizzaBlock\\LoadingBlock.jsx",[],["112","113"],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\components\\PizzaBlock\\index.jsx",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\reducers\\cart.js",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\redux\\action\\cart.js",[],"C:\\html\\JS\\ReactJS\\react-pizza\\src\\components\\CartItem.jsx",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"114","replacedBy":"118"},{"ruleId":"116","replacedBy":"119"},{"ruleId":"114","replac